{
    "name": "jamkit-tools",
    "displayName": "Jamkit Tools",
    "description": "The offical tools for Jamkit",
    "version": "0.1.0",
    "publisher": "bookjam",
    "engines": {
        "vscode": "^1.32.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "sbss",
                "extensions": [
                    ".sbss"
                ],
                "configuration": "./sbss-configuration.json"
            },
            {
                "id": "sbml",
                "extensions": [
                    ".sbml"
                ],
                "configuration": "./sbml-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "sbss",
                "scopeName": "source.sbss",
                "path": "./syntaxes/sbss.tmLanguage.json"
            },
            {
                "language": "sbml",
                "scopeName": "source.sbml",
                "path": "./syntaxes/sbml.tmLanguage.json"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/bookjam/vscode-jamkit.git"
    }
}