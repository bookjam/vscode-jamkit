{
    "name": "vscode-jamkit",
    "displayName": "Jamkit",
    "description": "Jamkit support for Visual Studio Code ",
    "version": "0.1.4",
    "publisher": "bookjam",
    "engines": {
        "vscode": "^1.32.0"
    },
    "categories": [
        "Programming Languages",
        "Other"
    ],
    "contributes": {
        "languages": [
            {
                "id": "bon",
                "extensions": [
                    ".bon"
                ],
                "configuration": "./bon-configuration.json"
            },
            {
                "id": "sbss",
                "extensions": [
                    ".sbss"
                ],
                "configuration": "./sbss-configuration.json"
            },
            {
                "id": "sbml",
                "extensions": [
                    ".sbml"
                ],
                "configuration": "./sbml-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "bon",
                "scopeName": "source.bon",
                "path": "./syntaxes/bon.tmLanguage.json"
            },
            {
                "language": "sbss",
                "scopeName": "source.sbss",
                "path": "./syntaxes/sbss.tmLanguage.json"
            },
            {
                "language": "sbml",
                "scopeName": "source.sbml",
                "path": "./syntaxes/sbml.tmLanguage.json"
            }
        ],
        "configurationDefaults": {
            "[sbss]": {
                "editor.wordBasedSuggestions": false
            },
            "[sbml]": {
                "editor.wordBasedSuggestions": false
            }
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/bookjam/vscode-jamkit.git"
    },
    "activationEvents": [
        "onLanguage:sbss",
        "onLanguage:sbml"
    ],
    "main": "./out/extension.js",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "lint": "eslint src --ext ts",
        "pretest": "npm run compile && npm run lint",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^8.0.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "^16.11.7",
        "@types/vscode": "^1.74.0",
        "@typescript-eslint/eslint-plugin": "^5.50.0",
        "@typescript-eslint/parser": "^5.50.0",
        "@vscode/test-electron": "^2.2.0",
        "eslint": "^8.28.0",
        "glob": "^8.0.3",
        "mocha": "^10.1.0",
        "typescript": "^4.9.3"
    },
    "dependencies": {
        "acorn": "^8.8.2"
    }
}
