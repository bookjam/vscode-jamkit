{
    "scopeName": "source.sbml",
    "patterns": [
        {
            "include": "#comment"
        },
        {
            "include": "#import"
        },
        {
            "include": "#begin"
        },
        {
            "include": "#end"
        },
        {
            "include": "#object"
        },
        {
            "include": "#if"
        },
        {
            "include": "#else"
        },
        {
            "include": "#inline-object"
        },
        {
            "include": "#inline-style"
        },
        {
            "include": "#anchor"
        }
    ],
    "repository": {
        "comment": {
            "match": "^\\s*=comment\\s+.*$",
            "name": "__STYLE_COMMENT__"
        },
        "import": {
            "match": "^\\s*(=import)\\s+(.+)$",
            "captures": {
                "1": {
                    "name": "__STYLE_SBML_IMPORT__"
                },
                "2": {
                    "name": "__STYLE_FILENAME__"
                }
            }
        },
        "begin": {
            "begin": "^\\s*(=begin)\\b([^:]+)?",
            "beginCaptures": {
                "1": {
                    "name": "__STYLE_SBML_BEGIN__"
                },
                "2": {
                    "name": "__STYLE_SBML_SECTION_NAME__"
                }
            },
            "end": "$",
            "patterns": [
                {
                    "include": "#property-list"
                }
            ]
        },
        "end": {
            "match": "^\\s*(=end)(\\s+(.+))?",
            "captures": {
                "1": {
                    "name": "__STYLE_SBML_END__"
                },
                "3": {
                    "name": "__STYLE_SBML_SECTION_NAME__"
                }
            }
        },
        "object": {
            "begin": "^\\s*(=(object|image))\\s+([^:]+)",
            "beginCaptures": {
                "1": {
                    "name": "__STYLE_SBML_BLOCK_OBJECT__"
                },
                "3": {
                    "name": "__STYLE_SBML_OBJECT_TYPE__"
                }
            },
            "end": "$",
            "patterns": [
                {
                    "include": "#property-list"
                }
            ]
        },
        "if": {
            "begin": "^\\s*=(el)?if\\s+",
            "beginCaptures": {
                "0": {
                    "name": "__STYLE_SBML_IF_ELIF__"
                }
            },
            "end": "$",
            "patterns": [
                {
                    "include": "#expression"
                }
            ]
        },
        "else": {
            "name": "__STYLE_SBML_ELSE__",
            "match": "^\\s*=else"
        },
        "inline-object": {
            "begin": "=\\((object|image)\\s+([^:]+)",
            "beginCaptures": {
                "0": {
                    "name": "__STYLE_SBML_INLINE_OBJECT__"
                },
                "2": {
                    "name": "__STYLE_SBML_OBJECT_TYPE__"
                }
            },
            "end": "\\)=",
            "endCaptures": {
                "0": {
                    "name": "__STYLE_SBML_INLINE_OBJECT__"
                }
            },
            "patterns": [
                {
                    "include": "#property-list"
                }
            ]
        },
        "inline-style": {
            "begin": "=\\[(.*?)\\|",
            "beginCaptures": {
                "0": {
                    "name": "__STYLE_SBML_STYLE_MARK__"
                },
                "1": {
                    "name": "__STYLE_SBML_STYLE_NAME__"
                }
            },
            "end": "\\]=",
            "endCaptures": {
                "0": {
                    "name": "__STYLE_SBML_STYLE_MARK__"
                }
            },
            "patterns": [
                {
                    "include": "#inline-style"
                },
                {
                    "include": "#inline-object"
                },
                {
                    "include": "#anchor"
                }
            ]
        },
        "anchor": {
            "match": "=\\(anchor\\|(.*)\\)=",
            "name": "__STYLE_SBML_ANCHOR_MARK__",
            "captures": {
                "1": {
                    "name": "__STYLE_SBML_ANCHOR_NAME__"
                }
            }
        },
        "property-list": {
            "patterns": [
                "__PROP_LIST_PATTERNS__"
            ]
        },
        "expression": {
            "patterns": [
                {
                    "include": "#operator"
                },
                {
                    "include": "#variable"
                },
                {
                    "include": "#number"
                }
            ]
        },
        "operator": {
            "patterns": [
                {
                    "match": "(==|\\!=|\\*|\\+|-|/|<|<=|>|>=)",
                    "name": "__STYLE_OPERATOR__"
                }
            ]
        },
        "variable": {
            "patterns": [
                {
                    "name": "__STYLE_VARIABLE__",
                    "match": "\\$[A-Z_]+"
                }
            ]
        },
        "number": {
            "patterns": [
                {
                    "match": "[+-]?([0-9]*[.])?[0-9]+",
                    "name": "__STYLE_NUMBER__"
                }
            ]
        }
    }
}